    "webpack-cli": "^4.2.0",
    "webpack-dev-middleware": "^4.0.2",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-node-externals": "^2.5.2",
    "ws": "4.0.0"
  },
  "engines": {
    "node": "16.x"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters":{
      "/**/*.{js,jsx}": [
        "prettier --write",
        "git add"
      ]
    },
    "ignore": ["node_modules", "dist", "package-lock.json"]
    
  }
}
