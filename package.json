    "lodash": "^4.17.20",
    "mongoose": "^5.10.15",
    "nanoid": "^3.1.30",
    "qrscan": "^1.0.6",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-qr-reader": "github:JodusNodus/react-qr-reader#pull/192/head",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-ssr-prepass": "^1.4.0",
    "sleep": "^6.3.0",
